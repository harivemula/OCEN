{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "Core/schema/Version_0.0.1/transaction.json", 
	"type": "object",
	"required": [
		"date",
		"narration",
		"txnRefNo",
		"amount",
		"type",
		"closingBalance"
	],
	"properties": {
		"date": { 
			"type": "string"
		},
		"narration": {
			"type": "string"
		},
		"txnRefNo": {
			"type": "string"
		},
		"amount": { 
			"type": "string"
		},
		"type": { 
			"type": "string",
			"enum": [
				"DEPOSIT","WITHDRAWL"
			]
		},
		"closingBalance": {
			"type": "string"
		},
		"url":{
			"type":"string"
		},
		"extensibleData":{
			"$ref": "extensibleData.json"
		}
	}
}
