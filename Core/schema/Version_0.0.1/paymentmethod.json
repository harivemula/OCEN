{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "Core/schema/Version_0.0.1/paymentmethod.json", 
	"type": "object",
	"required": [
		"id",
		"status"
	],
	"properties": {
		"id": {
			"type": "string"
		},
		"description": { 
			"type": "string"
		},
		"type": {
            "type": "string",
            "enum": ["EMANDATE_UPI","ELIEN","ENACH","ANY"]
		},
		"status": {
            "type": "string",
            "enum": ["INACTIVE","ACTIVE","CANCELLED","PAUSE"]

		},
		"data": {
			"$ref": "extensibleData.json"
		},
		"url": {
			"type": "string"
		},
		"extensibleData":{
			"$ref": "extensibleData.json"
		}
	}
}
