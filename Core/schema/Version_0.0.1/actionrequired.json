{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "Core/schema/Version_0.0.1/actionrequired.json",  
	"type": "object",
	"required": [
		"actionType",
		"description",
		"reference"
	],
	"properties": {
		"actionType": { 
			"type": "string",
			"enum": [
				"ADD_DOCUMENT","RESUBMIT_DOCUMENT","OTHER"
			]
		},
		"description": { 
			"type": "string",
			"examples": [
				"DL number not visible"
			]
		},
		"reference": { 
			"$ref": "reference.json"
		},
		"url":{
			"type":"string"
		},
		"extensibleData":{
			"$ref": "extensibleData.json"
		}

	}
}
 